{
  "version": 2,
  "framework": "vite",
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  },
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 